{
  "expo": {
    "name": "Facelinked",
    "slug": "Facelinked",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "newArchEnabled": true,
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.orion.friendslinked"
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.orion.facelinked",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ic_launcher_foreground.png",
        "backgroundColor": "#285FF5"
      },
      "splash": {
        "image": "./assets/images/icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/icon-rounded.png",
      "description": "Chat, Connect, and Share with Friends",
      "name": "Facelinked",
      "shortName": "Facelinked",
      "backgroundColor": "#ffffff",
      "themeColor": "#ffffff"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-build-properties",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
